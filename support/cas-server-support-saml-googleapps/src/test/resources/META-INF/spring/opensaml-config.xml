    <util:list id="inMemoryRegisteredServices">
        <bean class="org.apereo.cas.services.RegexRegisteredService"
              p:id="100" p:name="Test" p:description="Another Test Service"
              p:serviceId="^.+" p:evaluationOrder="100"/>
